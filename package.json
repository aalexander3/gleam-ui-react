{
  "name": "gleam-ui-react",
  "version": "1.0.3",
  "description": "gleam-ui-react React UI Kit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "nwb serve-react-demo",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/styles/ -o src/styles",
    "build-js": "nwb build-react-component --copy-files",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/styles --watch --recursive",
    "clean": "nwb clean-module && nwb clean-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "nwb": "0.23.x",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ]
}
